{
  "name": "biweb-cube",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "lint:eslint": "eslint --cache \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write .",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "prepare": "husky install",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "serve": "vite preview",
    "build:dev": "vue-tsc --noEmit --skipLibCheck && vite build --mode development",
    "build:test": "vue-tsc --noEmit --skipLibCheck && vite build --mode test",
    "build:sandbox": "vue-tsc --noEmit --skipLibCheck && vite build --mode sandbox",
    "build:pre": "vue-tsc --noEmit --skipLibCheck && vite build --mode pre",
    "build:prod": "vue-tsc --noEmit --skipLibCheck && vite build --mode production"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@types/nprogress": "^0.2.0",
    "@vueuse/core": "^6.5.3",
    "add": "^2.0.6",
    "ant-design-vue": "3.1.1",
    "axios": "^0.21.4",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.0",
    "js-cookie": "^3.0.1",
    "js-md5": "^0.7.3",
    "js-sha1": "^0.6.0",
    "lib-flexible": "^0.3.2",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.13",
    "postcss-px-to-viewport": "^1.1.1",
    "swiper": "7.2",
    "video.js": "^7.17.0",
    "videojs-contrib-hls": "^5.15.0",
    "vite-plugin-html": "^2.1.2",
    "vue": "^3.2.6",
    "vue-i18n": "^9.1.2",
    "vue-request": "^1.2.3",
    "vue-router": "^4.0.12",
    "vue-video-player": "^5.0.2",
    "xlsx": "^0.17.3",
    "yarn": "^1.22.15"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/js-cookie": "^2.2.7",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.11.26",
    "@types/path-to-regexp": "^1.7.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-legacy": "^1.6.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@vitejs/plugin-vue": "^1.6.1",
    "@vue/compiler-sfc": "^3.2.6",
    "amfe-flexible": "^2.2.1",
    "autoprefixer": "^10.3.4",
    "clean-publish": "^2.2.0",
    "cssnano": "^5.0.8",
    "cssnano-preset-advanced": "^5.1.4",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.2.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "husky": "^7.0.4",
    "less": "^4.1.1",
    "lint-staged": "^12.2.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "path-to-regexp": "^6.2.0",
    "postcss": "^8.3.11",
    "postcss-assets": "^6.0.0",
    "postcss-focus": "^5.0.1",
    "postcss-html": "^1.3.0",
    "postcss-import": "^14.0.2",
    "postcss-normalize": "^10.0.1",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "2.5.1",
    "px2rem-loader": "^0.1.9",
    "qs": "^6.10.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "stylelint": "^14.2.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "typescript": "^4.3.2",
    "unplugin-icons": "^0.11.4",
    "unplugin-vue-components": "^0.15.2",
    "vite": "^2.6.12",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vite-plugin-style-import": "^1.4.1",
    "vite-plugin-svg-icons": "^1.0.5",
    "vite-plugin-windicss": "^1.7.0",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.3.0",
    "windicss": "^3.4.3"
  },
  "lint-staged": {
    "*.{vue,ts,tsx}": "eslint --cache --fix",
    "**/*": "prettier --write --ignore-unknown",
    "*.{vue,less,postcss,css,scss}": "stylelint --cache --fix"
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@ant-design/icons-vue",
        "@vueuse/core",
        "ant-design-vue",
        "ant-design-vue/es",
        "ant-design-vue/es/button/style/css",
        "ant-design-vue/es/config-provider/style/css",
        "ant-design-vue/es/form/style/css",
        "ant-design-vue/es/input/style/css",
        "ant-design-vue/es/locale/zh_CN",
        "ant-design-vue/es/menu/style/css",
        "ant-design-vue/es/modal/style/css",
        "ant-design-vue/es/popover/style/css",
        "ant-design-vue/es/tabs/style/css",
        "ant-design-vue/es/tooltip/style/css",
        "axios",
        "crypto-js/aes",
        "crypto-js/enc-base64",
        "crypto-js/enc-utf8",
        "crypto-js/md5",
        "crypto-js/mode-ecb",
        "crypto-js/pad-pkcs7",
        "crypto-js/sha1",
        "js-cookie",
        "js-sha1",
        "lodash-es",
        "pinia",
        "qs",
        "swiper",
        "swiper/vue",
        "vue",
        "vue-i18n",
        "vue-router"
      ]
    }
  }
}
